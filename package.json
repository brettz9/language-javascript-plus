{
	"name": "language-javascript-plus",
	"version": "3.0.0",
	"description": "Syntax highlighting for template strings in Atom",
	"engines": {
		"atom": "*"
	},
	"scripts": {
		"rm": "rm -rf ~/.atom/dev/packages/language-javascript-plus",
		"cp": "cp -r ../language-javascript-plus ~/.atom/dev/packages/.",
		"l": "npm run rm && npm run cp && atom --clear-window-state --dev ."
	},
	"author": "Alexander Elias",
	"contributors": [
		"Brett Zamir"
	],
	"license": "MPL-2.0",
	"main": "index.js",
	"directories": {
		"test": "test"
	},
	"keywords": [
		"language",
		"javascript",
		"plus",
		"template",
		"literals",
		"syntax",
		"highliting"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:vokeio/language-javascript-plus.git"
	},
	"bugs": {
		"url": "https://github.com/vokeio/language-javascript-plus/issues"
	},
	"homepage": "https://github.com/vokeio/language-javascript-plus#readme",
	"dependencies": {
		"tree-sitter-javascript": "^0.15.1"
	}
}
